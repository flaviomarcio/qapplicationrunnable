{
   "arguments":"${APPLICATION_ARGS}",
   "protocol":{
      "default":{
         "minThreads":4,
         "maxThreads":2000,
         "cleanupInterval":1000,
         "readTimeout":60000,
         "maxRequestSize":104857600,
         "maxMultiPartSize":1048576000,
         "enabled":false,
         "sslKeyFile":"",
         "sslCertFile":""
      },
      "http":{
         "enabled":true,
         "contextPath":"${APPLICATION_CONTEXT_PATH}",
         "port":"${APPLICATION_PORT}"
      }
   },
   "apr":{
      "version":"1.0.0",
      "name":"${APR_NAME}",
      "host":{
         "protocol":"${APR_PROTOCOL}",
         "hostName":"${APR_DNS}",
         "port":"${APR_PORT}",
         "headers":{
            "Content-Type":"application/json",
            "Authorization":"Service ${APR_AUTH}"
         },
         "basePath":"${APR_CONTEXT_PATH}"
      }
   },
   "connection":{
      "environment":"debug",
      "paramaters":{
         "debug":{
            "driver":"${DB_DRIVER}",
            "hostName":"${DB_HOST}",
            "userName":"${DB_USER}",
            "password":"${DB_PASSWORD}",
            "port":"${DB_PORT}",
            "dataBaseName":"${DB_DATABASE}",
            "schemaNames":"${DB_SCHEMA}"
         }
      }
   },
   "services":{
      "default":{
         "protocol":"${APR_PROTOCOL}",
         "enabled":true
      },
      "circuit-breaker":{
         "activityLimit":"${APPLICATION_CIRCUIT_BREAKER_INTERVAL}",
         "enabled":"${APPLICATION_CIRCUIT_BREAKER_ENABLED}"
      },
      "environments":{
         "enabled":"${ENV_ENABLED}",
         "protocol":"${ENV_PROTOCOL}",
         "hostName":"${ENV_DNS}",
         "port":"${ENV_PORT}",
         "basePath":"${ENV_CONTEXT_PATH}"
      },
      "scheduler":{
         "scope":"${SCH_SCOPE}",
         "enabled":"${SCH_ENABLED}",
         "protocol":"${SCH_PROTOCOL}",
         "hostName":"${SCH_DNS}",
         "port":"${SCH_PORT}",
         "headers":{
            "Content-Type":"application/json",
            "authorization":"Service ${SCH_AUTH}"
         },
         "basePath":"${SCH_CONTEXT_PATH}"
      },
      "agent":{
         "enabled":"${AGT_ENABLED}",
         "protocol":"${AGT_PROTOCOL}",
         "hostName":"${AGT_DNS}",
         "port":"${AGT_PORT}",
         "headers":{
            "Content-Type":"application/json",
            "authorization":"Service ${AGT_AUTH}"
         },
         "basePath":"${AGT_CONTEXT_PATH}",
         "variables":{
            "strategy_uuid":"{a6992776-1931-4995-9f3c-57b03e9c30e2}"
         }
      },
      "token":{
         "activityLimit":"1y",
         "variables":{
            "secret":"ZG1Mx0OD-Uw^OW>Yw"
         }
      },
      "acl":{
         "enabled":"${ACL_ENABLED}",
         "protocol":"${ACL_PROTOCOL}",
         "hostName":"${ACL_DNS}",
         "port":"${ACL_PORT}",
         "headers":{
            "Content-Type":"application/json",
            "authorization":"Service ${ACL_AUTH}"
         },
         "basePath":"${ACL_CONTEXT_PATH}"
      },
      "message":{
         "enabled":"${MSG_ENABLED}",
         "protocol":"${MSG_PROTOCOL}",
         "hostName":"${MSG_DNS}",
         "port":"${MSG_PORT}",
         "headers":{
            "Content-Type":"application/json",
            "authorization":"Service ${MSG_AUTH}"
         },
         "basePath":"${MSG_CONTEXT_PATH}",
         "configurations":{
            "code":{
               "logMessage":true,
               "enabled":true,
               "length":4,
               "attempts":6,
               "dispatcher":false,
               "defaults":{
                  "enabled":true,
                  "type":"fake",
                  "number":"123456"
               },
               "compliance":{
                  "enabled":true,
                  "identification":"99123456789",
                  "number":"6545"
               }
            }
         }
      },
      "payment":{
         "enabled":"${PAY_ENABLED}",
         "protocol":"${PAY_PROTOCOL}",
         "hostName":"${PAY_DNS}",
         "port":"${PAY_PORT}",
         "headers":{
            "Content-Type":"application/json",
            "authorization":"Service ${PAY_AUTH}"
         },
         "basePath":"${PAY_CONTEXT_PATH}"
      },
      "fileserver":{
         "enabled":"${FIL_ENABLED}",
         "protocol":"${FIL_PROTOCOL}",
         "hostName":"${FIL_DNS}",
         "port":"${FIL_PORT}",
         "headers":{
            "Content-Type":"application/json",
            "authorization":"Service ${FIL_AUTH}"
         },
         "basePath":"${FIL_CONTEXT_PATH}"
      },
      "printservice":{
         "enabled":"${PTR_ENABLED}",
         "protocol":"${PTR_PROTOCOL}",
         "hostName":"${PTR_DNS}",
         "port":"${PTR_PORT}",
         "headers":{
            "Content-Type":"application/json",
            "authorization":"Service ${PTR_AUTH}"
         },
         "basePath":"${PTR_CONTEXT_PATH}"
      },
      "printserver":{
         "enabled":"${PTR_ENABLED}",
         "protocol":"http",
         "headers":{
            "Content-Type":"application/x-www-form-urlencoded"
         },
         "variables":{
            "links":{
               "login":{
                  "protocol":"http",
                  "hostName":"localhost",
                  "method":"POST",
                  "headers":{
                     "Content-Type":"application/x-www-form-urlencoded"
                  },
                  "route":"/jasperserver/rest_v2/reports/erp/erpv1/jasperserver/rest_v2/login",
                  "body":"j_username=erpv1&j_password_pseudo=erpv1&j_password=erpv1",
                  "body_":"j_username=${username}&j_password_pseudo=${username}&j_password=${password}"
               },
               "print":{
                  "protocol":"http",
                  "hostName":"localhost",
                  "method":"GET",
                  "headers":{
                     "Content-Type":"application/x-www-form-urlencoded"
                  },
                  "route":"/jasperserver/rest_v2/reports/erp/erpv1/${reportname}"
               }
            }
         }
      }
   }
}
